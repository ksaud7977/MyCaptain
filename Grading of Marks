#include<stdio.h>
int main()
{
	int x;
	printf("Enter the Marks\n");
	scanf("%d",&x);
	if(85<=x && x<=100)
	printf("Grade A\n");
	else if(70<=x && x<=84)
	printf("Grade B\n");
	else if(55<=x && x<=69)
	printf("Grade C\n");
	else if(40<=x && x<=54)
	printf("Grade D");
	else if(x<=54)
	printf("Grade E\n");
	else
	printf("Wrong Input");
	return 0;
}
