#include<stdio.h>
int main()
{
	int n;
	scanf("%d",&n);
	switch(n)
	{
		case 1:
		printf("Food Item ~ Pizza\n");
		printf("Price is Rs 239");
		break;
		case 2:
		printf("Food Item ~ Burger\n");
		printf("Price is Rs 129");
		break;
		case 3:
		printf("Food Item ~ Pasta\n");
		printf("Price is Rs 179");
		break;
		case 4:
		printf("Food Item ~ French Fries\n");
		printf("Price is Rs 99");
		break;
		case 5:
		printf("Food Item ~ Sandwich\n");
		printf("Price is Rs 149");
		break;
		default:
		printf("Item not in the list");
		break;	
	}
	getch();
}
